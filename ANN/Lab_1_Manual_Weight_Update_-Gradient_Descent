 "cells": [
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "# X & y \n",
    "# X - Study hours\n",
    "# y - score in exam \n",
    "x_data = [1.0, 2.0, 3.0]\n",
    "y_data = [2.0, 4.0, 6.0]\n",
    "\n",
    "# randmly\n",
    "b =  1.0\n",
    "a =  1.0\n",
    "lr = 0.001\n",
    "\n",
    "\n",
    "#forward pass a + bx \n",
    "def forward(x):\n",
    "    return x * b + a\n",
    "\n",
    "# Loss function\n",
    "def loss(x, y):\n",
    "    y_pred = forward(x)\n",
    "    return 0.5*(y_pred - y)**2\n",
    "\n",
    "\n",
    "# compute gradient\n",
    "def gradient_b(x, y):  # d_loss/d_w\n",
    "    return x * (x * b - y)\n",
    "\n",
    "def gradient_a(x, y):  # d_loss/d_w\n",
    "    return (x * b) - y\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
