   "source": [
    "# training loop\n",
    "for epoch in range(1000):    \n",
    "    for x_val, y_val in zip(x_data, y_data):\n",
    "        grad_b = gradient_b(x_val, y_val)\n",
    "        grad_a = gradient_a(x_val, y_val)\n",
    "        b = b - lr * grad_b\n",
    "        a = a - lr * grad_a\n",
    "        l = loss(x_val, y_val)\n",
    "    print(\"Epoch:\", epoch, \"b=\", round(b, 2), \"a=\", round(a, 2),\"loss=\", round(l, 2))"
   ]
  },
